@article{petersen1994benefits,
	Abstract = { This paper empirically examines how ties between a firm and its creditors affect the availability and cost of funds to the firm. The authors analyze data collected in a survey of small firms by the Small Business Administration. The primary benefit of building close ties with an institutional creditor is that the availability of financing increases. The authors find smaller effects on the price of credit. Attempts to widen the circle of relationships by borrowing from multiple leaders increases the price and reduces the availability of credit. In sum, relationships are valuable and appear to operate more through quantities rather than prices. Copyright 1994 by American Finance Association.},
