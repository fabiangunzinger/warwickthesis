@article{tervio2008difference,
	Abstract = {This paper presents an assignment model of CEOs and firms. The distributions of CEO pay levels and firms' market values are analyzed as the competitive equilibrium of a matching market where talents, as well as CEO positions, are scarce. It is shown how the observed joint distribution of CEO pay and market value can then be used to infer the economic value of underlying ability differences. The variation in CEO pay is found to be mostly due to variation in firm characteristics, whereas implied differences in managerial ability are small and make relatively little difference to shareholder value.},
	Author = {Marko Tervi{\"o}},
